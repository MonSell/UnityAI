<Brain>
	<Memory>
		<Variable name="MoveTarget" type="GameObject" />
	</Memory>
	<BehaviorTree>
		<Selector>
			<Sequence>
				<FindGameObjectByName name="Destination" variable="MoveTarget" />
				<Move to="MoveTarget" />
				<Speak message="I reached the destination." />
				<Halt />
			</Sequence>
			<Sequence>
				<Speak message="Aaaargh! I can't reach the destination." />
				<Halt />
			</Sequence>
		</Selector>
	</BehaviorTree>
</Brain>

